{
  "staging": {
    "api_base_url": "https://stg-app.bosta.co/api/v2",
    "business_base_url": "https://stg-business.bosta.co",
    "environment": "staging",
    "debug": true,
    "ssl_verify": true,
    "default_headers": {
      "origin": "https://stg-business.bosta.co",
      "referer": "https://stg-business.bosta.co/"
    }
  },
  "production": {
    "api_base_url": "https://app.bosta.co/api/v2",
    "business_base_url": "https://business.bosta.co",
    "environment": "production",
    "debug": false,
    "ssl_verify": true,
    "default_headers": {
      "origin": "https://business.bosta.co",
      "referer": "https://business.bosta.co/"
    }
  },
  "security_testing": {
    "enable_dangerous_tests": true,
    "max_concurrent_requests": 10,
    "request_delay": 0.1,
    "bypass_rate_limits": false,
    "log_all_requests": true,
    "save_payloads": true
  },
  "ai_config": {
    "openai_api_key": "${OPENAI_API_KEY}",
    "anthropic_api_key": "${ANTHROPIC_API_KEY}",
    "enable_ai_analysis": true,
    "ai_model_preference": "gpt-4"
  }
}
